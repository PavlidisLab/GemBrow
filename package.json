{
  "name": "gembrow",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "devbuild": "vue-cli-service build --mode=development",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@mdi/font": "^7.1.96",
    "axios": "^0.27.2",
    "json2csv": "^5.0.7",
    "lodash": "^4.17.21",
    "material-icons": "^1.13.1",
    "qs": "^6.11.0",
    "vue": "^2.7.14",
    "vue-router": "^3.6.5",
    "vuetify": "^2.6.14",
    "vuex": "^3.6.2",
    "vuex-persistedstate": "^4.1.0",
    "vuex-rest-api": "^2.15.0"
  },
  "devDependencies": {
    "@vue/cli": "^5.0.8",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^7.0.0",
    "eslint": "^8.33.0",
    "eslint-plugin-vue": "^9.9.0",
    "eslint-plugin-vuetify": "^1.1.0",
    "lint-staged": "^13.1.0",
    "sass": "^1.58.0",
    "sass-loader": "^13.2.0",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^5.75.0"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "vue/multi-word-component-names": "off",
      "prettier/prettier": "off",
      "comma-dangle": [
        "error",
        "never"
      ],
      "vue/valid-v-slot": [
        "error",
        {
          "allowModifiers": true
        }
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "defaults"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}

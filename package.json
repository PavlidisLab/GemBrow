{
  "name": "gembrow",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@mdi/font": "^6.4.95",
    "axios": "^0.21.0",
    "babel-polyfill": "^6.26.0",
    "json2csv": "^5.0.6",
    "material-icons": "^1.10.0",
    "moment": "^2.29.1",
    "vue": "^2.6.14",
    "vue-router": "^3.5.3",
    "vuetify": "^1.5.24",
    "vuex": "^3.6.2",
    "vuex-persistedstate": "^4.1.0",
    "vuex-rest-api": "^2.14.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@vue/cli": "^4.5.15",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/eslint-config-prettier": "^6.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^8.0.3",
    "lint-staged": "^6.1.1",
    "sass": "^1.43.4",
    "sass-loader": "^10.2.0",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.61.0"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "vue/multi-word-component-names": "off",
      "prettier/prettier": "off",
      "comma-dangle": ["error", "never"]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
